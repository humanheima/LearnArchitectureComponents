<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.example.android.jetpackdemo">


    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
            android:name="com.example.android.jetpackdemo.App"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name=".activityresult.TargetActivity"
                android:exported="false" />
        <activity
                android:name=".activityresult.GetActivityResultActivity"
                android:exported="false" />
        <activity android:name="com.example.android.jetpackdemo.page.PageActivity" />
        <activity android:name="com.example.android.jetpackdemo.lifecycle.LifeCycleActivity" />
        <activity android:name="com.example.android.jetpackdemo.ExampleActivity" />
        <activity android:name="com.example.android.jetpackdemo.room.NewWordActivity" />
        <activity android:name="com.example.android.jetpackdemo.livedata.LiveDataActivity" />
        <activity android:name="com.example.android.jetpackdemo.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>